<!doctype html>
<html>
  <head>
    <title>Group7 Challenge5</title>
  </head>
  <body>
	<h1>Group 7 Challenge 5</h1>
	<p>Indoor positioning</p>
  
	<canvas id="myCanvas" width="440" height="180" style="border:1px solid #d3d3d3;">Your browser does not support the HTML5 canvas tag.</canvas>
    
    <script src="/socket.io/socket.io.js"></script>
	<script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
	<script src="http://code.jquery.com/jquery-1.11.1.js"></script>
	<script>
	  var socket = io();

	  socket.on('Coordinate Message', function(msg){
		var X = parseFloat(msg.substring(1, 3));
		var Y = parseFloat(msg.substring(5, 7));
		//alert("x" + X + "   y" + Y);
		
		var c = document.getElementById("myCanvas");
		var ctx = c.getContext("2d");
		ctx.beginPath();
		ctx.clearRect(0,0,440,180);
		ctx.arc(X,Y,5,0,2*Math.PI);		
		ctx.stroke();	
	  });
	</script>
</body>
</html>
